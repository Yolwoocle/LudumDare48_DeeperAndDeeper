function _update60()
	if btn(rigth) then
		player.vx += 1
	end
	if btn(left) then
		player.vx -= 1
	end
	if btn(fire1) then
		deep += 1
	end
	
	player.vx *= 0.9
	player.vx *= 0.9
	player.x += player.vx
	player.y += player.vy
end

-->8
--draw 

function _draw()
	cls(dark_blue)
	map(0,deep,0,0,16,16)
	
	
	spr(2, player.x, player.y)
end



