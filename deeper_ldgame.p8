function no_update()
	if (btn(down)) then
		cam.deep += 1 --(((cam.deep+0.11)*10)\1)/10 --+(time()/3000)
		lock = false
	end
	if (cam.deep%8)==0 and cam.deep!=0 and not lock then
		lock = true
		setnew(cam.deep/8\1)
	end
end

