--MODIFICATION DE LA MAP CHAQUE 8 PIXEL
function setnew(n)
	for i=0,16 do 
		for j=0,16 do
			mset(i, j-1, mget(i,j))
		end
	end
	for i=0,16 do
		mset(i,16, 0)
		random_block = rnd(100) 
		block = 1
		if random_block < 1 then
			block = 10
		elseif random_block < 4 then
			block = random(4, 9)
		elseif random_block < 35 then
			block = 1--rnd({2, 3})
		elseif random_block < 45 then
			block = 1
		end 
		mset(i, 16, block)
	end
end

--GENERATION DE LA PREMIER MAP
function firstgenerationmap()
	for i=0,16 do 
		for j=0,17 do
			if random(1,4) == 1 then
				mset(i,j,1)
			end
		end
	end
end


function minimap_debug()
	for i=0,32 do 
		for j=0,32 do
			if mget(i,j) == 1 then
				pset(i,j,green)
			end
		end
	end
	
end
